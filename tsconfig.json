{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "skipDefaultLibCheck": true,
    "target": "ES6",
    "jsx": "react-native",
    "lib": ["dom", "esnext", "ES2019"],
    "allowJs": true,
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": false
  },
  "include": ["./src", "togeojson.d.ts", "navigation", "navigation"]
}
